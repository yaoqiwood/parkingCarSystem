.btn-group{
    position: fixed;
    bottom: 20px;
    left: 20px;
}

.inverse-btn{
    width: 120px;
    height:45px;
    background-color: #009789;
    border: #009789 0px solid;
    border-radius: 27px;
    color:white;
    text-align: center;
    line-height: 40px;
    cursor: pointer;
}


.inverse-btn:hover{
    background-color: orangered;
}

.carnum-plugin{
    position: absolute;
    left: 50%;
    top: 50%;    
    transform: translateX(-50%)translateY(-50%);
}

.btn-center{
    text-align: center;
}

.align-right{
    text-align: right;
}

.float-right{
    float: right;
}

.line-height-i{
    line-height: 22px;
}

.shadow_panel{
    position: fixed;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,.7);
    display: none;
    z-index: 3;
}

#close_btn{
    color:#626262;
    cursor: pointer;
}

#close_btn:hover{
    color:#333;
}