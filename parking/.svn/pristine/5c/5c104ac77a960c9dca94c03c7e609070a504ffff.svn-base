.addPanelBox{
    position: absolute;
    left: 50%;
    z-index: 3;;
    top: 20%;
    width: 400px;
    transform:translateX(-200px);
    display: none;
    border: 1px solid #3280FC;
    border-radius: 4px;
    padding-bottom: 0;
}

.updatePanelBox{
    position: absolute;
    left: 50%;
    z-index: 3;;
    top: 20%;
    width: 400px;
    transform:translateX(-200px);
    display: none;
    border: 1px solid #3280FC;
    border-radius: 4px;
}

.table-radius{
    border: 1px solid #ccc;
    border-radius: 4px;
}

.middle{
    vertical-align: middle;
}

.pager-label{
    line-height: 20px;
}

#myPager{
    margin-right: 2px;
}

.backWhite{
    display: none;
    position: absolute;
    width: 100%;
    height: 100%;
    top:0;
    left: 0;
    background-color: rgba(255, 255, 255, 0.2);
    z-index: 2;
}

.panel{
    border: 0;
	-webkit-box-shadow: 0 0 0 rgba(255, 255, 255, 0);
}

.bottomSpace{
    margin-bottom: 12px;
}

.scan {
	margin-bottom: 3px;
}

.scan .btn {
	width: 100%;
}


#table button{
    margin: 0 3px 2px 0;
}


.item-body ul{
	max-height: 200px;
	overflow: scroll;
	border: 1px solid black;
}

.item-body li{
	width: 132px;
	display: inline-block;
	margin: 0 3px 3px 0;
}

.itemPanelBox{
    position: absolute;
    left: 50%;
    z-index: 3;;
    top: 20%;
    width:480px;
    transform:translateX(-200px);
    display: none;
    border: 1px solid #3280FC;
    border-radius: 4px;
}
